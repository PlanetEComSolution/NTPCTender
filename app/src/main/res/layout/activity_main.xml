<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/linerfortime"
            android:layout_width="match_parent"
            android:layout_height="80sp"
            android:background="@color/colorPrimaryDark_2"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">


            <TextView
                android:id="@+id/textView1rr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:text="NTPC"
                android:textColor="@android:color/white" />


        </LinearLayout>

        <!--
                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/eusername1">-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/white"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingBottom="90dp"
            android:paddingLeft="50dp"
            android:paddingRight="50dp"
            android:paddingTop="90dp">

            <View
                android:layout_width="@dimen/view_width"
                android:layout_height="@dimen/view_width" />

            <LinearLayout
                android:id="@+id/ll_billable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"

                android:visibility="visible">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />

                    <LinearLayout
                        android:id="@+id/first"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"

                        android:background="@color/colorPrimaryDark_2"
                        android:gravity="center"

                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/selectvisd"
                            android:layout_width="55dp"
                            android:layout_height="55dp"


                            android:clickable="false"
                            android:src="@mipmap/ic_launcher" />

                        <TextView

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="View Live Tenders"

                            android:textColor="@android:color/white"

                            android:textSize="@dimen/textSize_homeActivity" />


                    </LinearLayout>

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />

                    <LinearLayout
                        android:id="@+id/first1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@color/colorPrimaryDark_2"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/selectvis1d"
                            android:layout_width="55dp"
                            android:layout_height="55dp"

                            android:clickable="false"
                            android:src="@mipmap/ic_launcher" />

                        <TextView

                            android:id="@+id/textView5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="Search Tenders"

                            android:textColor="@android:color/white"

                            android:textSize="@dimen/textSize_homeActivity" />


                    </LinearLayout>

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />
                </LinearLayout>

                <View
                    android:layout_width="@dimen/view_width"
                    android:layout_height="@dimen/view_width" />
            </LinearLayout>

            <!--new chnges-->

            <LinearLayout
                android:id="@+id/ll_billable_new"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"


                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />

                    <LinearLayout
                        android:id="@+id/ll_LocateCrate"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@color/colorPrimaryDark_2"
                        android:gravity="center"

                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/img_locateCrate"
                            android:layout_width="55dp"
                            android:layout_height="55dp"


                            android:clickable="false"
                            android:src="@mipmap/ic_launcher" />

                        <TextView

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:gravity="center"
                            android:text="share the App"
                            android:textColor="@android:color/white"

                            android:textSize="@dimen/textSize_homeActivity" />


                    </LinearLayout>

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />

                    <LinearLayout
                        android:id="@+id/ll_DamageReport"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@color/colorPrimaryDark_2"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/img_damageReport"
                            android:layout_width="55dp"
                            android:layout_height="55dp"

                            android:clickable="false"
                            android:src="@mipmap/ic_launcher" />

                        <TextView

                            android:id="@+id/text_damageReport"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="Other"

                            android:textColor="@android:color/white"

                            android:textSize="@dimen/textSize_homeActivity" />


                    </LinearLayout>


                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />
                </LinearLayout>

                <View
                    android:layout_width="@dimen/view_width"
                    android:layout_height="@dimen/view_width" />
            </LinearLayout>
